<% obj.each do |rsvp| %>
<div class="row">
  <div class="col-sm-10 ml-4" align="center">
    <div class="event-title">
      <%= link_to rsvp.event.title, event_path(rsvp.event) %>
      <div class="event-meta-details">
        <small>Created by: <%= rsvp.event.user.full_name%> <br>
          <%= time_ago_in_words(rsvp.event.created_at) %> ago</small>
      </div>
    </div>
  </div>
</div>
</div>
<% end %>